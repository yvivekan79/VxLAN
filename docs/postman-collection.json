    {
      "name": "Topology",
      "item": [
        {
          "name": "Create Hub-Spoke Topology",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"topology_type\": \"hub-spoke\",\n  \"nodes\": {\n    \"hub\": {\n      \"ip\": \"192.168.1.10\",\n      \"vni_base\": 1000\n    },\n    \"spokes\": [\n      {\"ip\": \"192.168.2.10\", \"name\": \"site-b\"},\n      {\"ip\": \"192.168.3.10\", \"name\": \"site-c\"}\n    ]\n  }\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/v1/topology",
